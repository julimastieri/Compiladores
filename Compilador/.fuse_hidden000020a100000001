.386
.model flat, stdcall
option casemap :none
include \masm32\include\windows.inc
include \masm32\include\kernel32.inc
include \masm32\include\user32.inc
includelib \masm32\lib\kernel32.lib
includelib \masm32\lib\user32.lib
.data
TituloCadena db "Cadena",0
_a DW ?
_b DW 1,2,3
_w DW ?
.code
start:
MOV AX,2
MOV _a,AX
MOV AX,1
MOV _w,AX
CMP _a,_w
JNE LabelElse
MOV AX,2
MOV _a,AX
MOV AX,12
MOV _w,AX
JMP LabelSiguiente
LabelElse:
MOV AX,10
MOV _w,AX
LabelSiguiente:
invoke ExitProcess, 0
end start
